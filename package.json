{
  "name": "qq-group-bot",
  "version": "1.8.0",
  "license": "MIT",
  "author": "Cansiny0320",
  "main": "index.js",
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc",
    "coverage": "vitest run --coverage",
    "delete": "pm2 delete bot",
    "dev": "nodemon ./src/index.ts",
    "fix": "gts fix",
    "lint": "gts lint",
    "lint-staged": "lint-staged",
    "list": "pm2 list",
    "log": "pm2 logs bot",
    "prepare": "husky install",
    "release": "zx scripts/release.mjs",
    "start": "npm run fix && npm run clean && npm run compile && pm2 start build/index.js --name=bot",
    "stop": "pm2 stop bot",
    "test": "vitest run",
    "type-check": "tsc --noEmit --skipLibCheck",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "axios-retry": "^3.2.4",
    "cheerio": "^1.0.0-rc.10",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "node-schedule": "^2.1.0",
    "oicq": "^2.2.0",
    "picocolors": "^1.0.0",
    "puppeteer": "^13.4.0",
    "qs": "^6.10.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/node-schedule": "^1.3.2",
    "@types/qs": "^6.9.7",
    "c8": "^7.11.0",
    "gts": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15",
    "pm2": "^5.2.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "vitest": "^0.5.7",
    "zx": "^5.2.0"
  }
}
