{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "ESNEXT",
    "rootDir": "./src",
    "outDir": "build",
    "moduleResolution": "node",
    "lib": ["DOM.Iterable", "DOM", "ESNext"],
    "esModuleInterop": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["src/**/tests/*.ts"]
}
